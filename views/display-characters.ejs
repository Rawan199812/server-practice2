<!DOCTYPE html>
<html lang="en">
 
    <%-include('./layouts/head' ,{page_title:'All Characters'})%>
    <body>
        <%-include('./layouts/nav')%>
    <main>

 

    <h2>You have  <%=results.length%>saved characters</h2>
    
    <%results.forEach(element=>{%>
        <section>
      
         <h1><%=element.name%></h1>
         <h1><%=element.house%></h1>
         <h1><%=element.patronus%></h1>
         <h1><%=element.is_alive%></h1>


         
           <a href="/character/<%=element.id%>">See Details</a>
        </section>
      
   
        <%})%>

</main>

</body>
</html>